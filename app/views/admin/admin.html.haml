                %td.sorting_1 Gecko
                %td Firefox 3.0
                %td Win 2k+ / OSX.3+
                %td 1.9
                %td A
              %tr.odd{:role => "row"}
                %td.sorting_1 Gecko
                %td Camino 1.0
                %td OSX.2+
                %td 1.8
                %td A
              %tr.even{:role => "row"}
                %td.sorting_1 Gecko
                %td Camino 1.5
                %td OSX.3+
                %td 1.8
                %td A
              %tr.odd{:role => "row"}
                %td.sorting_1 Gecko
                %td Netscape 7.2
                %td Win 95+ / Mac OS 8.6-9.2
                %td 1.7
                %td A
              %tr.even{:role => "row"}
                %td.sorting_1 Gecko
                %td Netscape Browser 8
                %td Win 98SE+
                %td 1.7
                %td A
              %tr.odd{:role => "row"}
                %td.sorting_1 Gecko
                %td Netscape Navigator 9
                %td Win 98+ / OSX.2+
                %td 1.8
                %td A
              %tr.even{:role => "row"}
                %td.sorting_1 Gecko
                %td Mozilla 1.0
                %td Win 95+ / OSX.1+
                %td 1
                %td A
            %tfoot
              %tr
                %th{:colspan => "1", :rowspan => "1"} Rendering engine
                %th{:colspan => "1", :rowspan => "1"} Browser
                %th{:colspan => "1", :rowspan => "1"} Platform(s)
                %th{:colspan => "1", :rowspan => "1"} Engine version
                %th{:colspan => "1", :rowspan => "1"} CSS grade
      .row
        .col-sm-5
          #example2_info.dataTables_info{"aria-live" => "polite", :role => "status"} Showing 1 to 10 of 57 entries
        .col-sm-7
          #example2_paginate.dataTables_paginate.paging_simple_numbers
            %ul.pagination
              %li#example2_previous.paginate_button.previous.disabled
