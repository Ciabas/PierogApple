                %th.sorting{"aria-controls" => "example2", "aria-label" => "CSS grade: activate to sort column ascending", :colspan => "1", :rowspan => "1", :tabindex => "0"} CSS grade
            %tbody
              %tr.odd{:role => "row"}
                %td.sorting_1 Gecko
                %td Firefox 1.0
                %td Win 98+ / OSX.2+
                %td 1.7
                %td A
              %tr.even{:role => "row"}
                %td.sorting_1 Gecko
                %td Firefox 1.5
                %td Win 98+ / OSX.2+
                %td 1.8
                %td A
              %tr.odd{:role => "row"}
                %td.sorting_1 Gecko
                %td Firefox 2.0
                %td Win 98+ / OSX.2+
                %td 1.8
                %td A
